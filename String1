// capacity(), resize() and shrink_to_fit()
#include<iostream>
#include<string>
using namespace std;

int main(){

string str;
getline(cin, str);
cout<<str<<endl;
str.resize(10);
cout<<str<<endl;

cout<<str.capacity()<<endl;

cout<<str.length()<<endl;

str.shrink_to_fit();
cout<<str.capacity()<<endl;
    return 0;
}
